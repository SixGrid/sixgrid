{
	"name": "esix-gui",
	"productName": "SixGrid",
	"version": "0.2.4",
	"description": "Open Source e621.net Browser",
	"main": "src/index.js",
	"scripts": {
		"start": "electron-forge start",
		"package": "electron-forge package",
		"make": "electron-forge make",
		"publish": "electron-forge publish",
		"build-installer": "electron-builder",
		"lint": "eslint src --color"
	},
	"keywords": [
		"e621",
		"esix-api",
		"esix-cli",
		"electron-forge",
		"electron",
		"standalone",
		"desktop app",
		"standalone app"
	],
	"build": {
		"appId": "club.jyles.sixgrid",
		"copyright": "Copyright Â© 2020-2021 SixGrid Contributers",
		"compression": "maximum",
		"artifactName": "sixgrid.{ext}",
		"win": {
			"target": [
				"nsis"
			],
			"icon": "src/sixgrid.ico"
		},
		"mac": {
			"target": [
				"dmg"
			],
			"category": "public.app-category.utilities",
			"icon": "src/sixgrid.icns"
		},
		"linux": {
			"target": [
				"appimage"
			],
			"category": "Network",
			"icon": "src/sixgrid.png"
		}
	},
	"nsis": {
		"installerIcon": "src/sixgrid.ico",
		"uninstallerIcon": "src/sixgrid.ico",
		"uninstallDisplayName": "SixGrid",
		"oneClick": false,
		"allowToChangeInstallationDirectory": true
	},
	"contributors": [
		"Jyles Coad-Ward <jyles@dariox.club> (https://jyles.club)",
		"zyme-xd (https://zyrn.wtf/)"
	],
	"author": "jyles.coad-ward",
	"license": "GPL-2.0-only",
	"config": {
		"forge": {
			"make_targets": {
				"win32": [
					"nsis"
				],
				"darwin": [
					"dmg"
				],
				"linux": [
					"appimage"
				]
			},
			"electronPackagerConfig": {
				"packageManager": "npm"
			},
			"electronWinstallerConfig": {
				"name": "esix-gui"
			},
			"electronInstallerDebian": {},
			"electronInstallerRedhat": {},
			"github_repository": {
				"owner": "jylescoad-ward",
				"name": "esix-gui"
			}
		}
	},
	"dependencies": {
		"@electron-forge/cli": "^6.0.0-beta.54",
		"child-process": "^1.0.2",
		"electron-compile": "^6.4.4",
		"electron-squirrel-startup": "^1.0.0",
		"esix-api": "^0.1.14",
		"fs": "^0.0.1-security",
		"jquery": "^3.5.1",
		"md5": "^2.3.0",
		"remove-markdown": "^0.3.0",
		"request": "^2.88.2",
		"sweetalert": "^2.1.2",
		"tinytoolbox": "0.0.7",
		"yarn": "^1.22.10"
	},
	"devDependencies": {
		"babel-plugin-transform-async-to-generator": "^6.24.1",
		"babel-preset-env": "^1.7.0",
		"babel-preset-react": "^6.24.1",
		"electron": "^11.1.0",
		"electron-builder": "^22.9.1",
		"electron-forge": "5.2.4",
		"electron-prebuilt-compile": "8.2.0"
	}
}
